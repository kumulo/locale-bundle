{
  "name": "kumulo/locale-bundle",
  "description": "Tool for providing and return locale with headers",
  "keywords": [],
  "homepage": "https://github.com/kumulo/locale-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Lanet"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "symfony/event-dispatcher": "^3.4",
    "symfony/http-kernel": ">=4.0",
    "symfony/http-foundation": ">=4.0"
  },
  "require-dev": {
    "symfony/serializer": "^4.4|^5.0"
  },
  "autoload": {
    "psr-4": { "Kumulo\\Bundle\\LocaleBundle\\": "src/" },
    "exclude-from-classmap": [
      "src/Tests/"
    ]
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "master"
    }
  }
}
